{
  "agent": {
    "watchers": [
      {
        "watch": "sample/log.txt",
        "tags": [
          {
            "key": "Name",
            "value": "Sample Log"
          },
          {
            "key": "Type",
            "value": "filesystem"
          }
        ]
      },
      {
        "watch": "sample/log2.txt",
        "tags": [
          {
            "key": "Name",
            "value": "Sample Log 2"
          },
          {
            "key": "Type",
            "value": "filesystem"
          }
        ]
      },
      {
        "watch": "sample/log3.txt",
        "tags": [
          {
            "key": "Name",
            "value": "Sample Log 3"
          },
          {
            "key": "Type",
            "value": "service"
          }
        ]
      }
    ],
    "max_retries": 3,
    "max_queue_size": 1000,
    "timeout": "5s"
  },
  "server": {
    "host": "127.0.0.1",
    "port": 33555,
    "store": "s3",
    "bucket": "sample",
    "endpoint": "http://127.0.0.1:9000",
    "aws_access_key": "minioadmin",
    "aws_secret_key": "minioadmin",
    "path_addressing_mode": true
  }
}
